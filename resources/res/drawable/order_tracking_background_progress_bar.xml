<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/ds_xxs"/>
            <solid android:color="@color/ds_neutral_white"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:topLeftRadius="@dimen/ds_xxs" android:bottomLeftRadius="@dimen/ds_xxs"/>
                <solid android:color="@color/primary_005"/>
            </shape>
        </scale>
    </item>
</layer-list>
