            <LinearLayout android:gravity="center_vertical" android:id="@+id/rating_container" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/ds_xxxs" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="0">
