            <TextView android:textColor="@color/ds_neutral_100" android:id="@+id/deliveryModeLabel" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/ds_xxxs" android:paddingEnd="@dimen/ds_xxxs" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/flowContainer" style="@style/DSTextAppearance.Caption"/>
            <com.talabat.collectiondetails.ui.customviews.DelayBadge android:id="@+id/delayBadge" android:background="@drawable/bg_delay_badge" android:paddingLeft="@dimen/ds_xxxs" android:paddingTop="@dimen/badge_vertical_padding" android:paddingRight="@dimen/ds_xxxs" android:paddingBottom="@dimen/badge_vertical_padding" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/badge_vertical_padding" android:paddingHorizontal="@dimen/ds_xxxs" android:paddingVertical="@dimen/badge_vertical_padding" app:layout_constraintStart_toEndOf="@+id/deliveryModeLabel" app:layout_constraintTop_toBottomOf="@+id/flowContainer"/>
            <ImageView android:id="@+id/dashedImage" android:layout_width="match_parent" android:layout_height="2dp" android:layout_marginTop="@dimen/ds_xxxs" android:src="@drawable/bg_line_dashed" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/deliveryBarrier"/>
            <LinearLayout android:orientation="vertical" android:id="@+id/discountContainer" android:layout_width="0dp" android:layout_height="wrap_content" app:layout_constraintEnd_toStartOf="@+id/moreOffers" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/dashedImage">
                <TextView android:textColor="@color/ds_promo_100" android:ellipsize="end" android:id="@+id/textView_pickupDiscountTag" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/ds_xxxs" android:lines="1" android:drawablePadding="@dimen/ds_xxxs" android:drawableStart="@drawable/ic_offer_tag_listpx" android:textAlignment="viewStart" android:layout_marginEnd="@dimen/ds_xxxs" style="@style/TextAppearance.Body1"/>
                <TextView android:textColor="@color/ds_promo_100" android:ellipsize="end" android:id="@+id/textView_discountTag" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/ds_xxxs" android:lines="1" android:drawablePadding="@dimen/ds_xxxs" android:drawableStart="@drawable/ic_offer_tag_listpx" android:textAlignment="viewStart" android:layout_marginEnd="@dimen/ds_xxxs" style="@style/TextAppearance.Body1"/>
            </LinearLayout>
            <ImageView android:id="@+id/tmp_icon" android:layout_width="14dp" android:layout_height="14dp" android:layout_marginTop="@dimen/ds_xxxs" android:layout_marginEnd="@dimen/ds_xxxs" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/dashedImage" app:srcCompat="@drawable/ds_alert_filled" app:tint="@color/ds_alert_100"/>
            <TextView android:textColor="@color/ds_neutral_100" android:ellipsize="end" android:id="@+id/textView_tmp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/ds_xxxs" android:text="@string/tmp_tag" android:lines="1" android:textAlignment="viewStart" android:layout_marginStart="@dimen/ds_xxxs" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintStart_toEndOf="@+id/tmp_icon" app:layout_constraintTop_toBottomOf="@+id/dashedImage" style="@style/TextAppearance.Body2"/>
            <TextView android:textColor="@color/ds_promo_100" android:gravity="center" android:id="@+id/moreOffers" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/ds_xxxs" android:drawablePadding="@dimen/ds_xxxs" android:layout_marginEnd="@dimen/ds_xxxs" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintEnd_toEndOf="0" style="@style/DSTextAppearance.Tag"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
