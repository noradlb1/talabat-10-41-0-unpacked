            </FrameLayout>
        </FrameLayout>
    </FrameLayout>
    <androidx.appcompat.widget.AppCompatImageButton android:id="@+id/favouriteButton" android:background="@drawable/bg_heart" android:padding="@dimen/ds_xxxs" android:visibility="gone" android:layout_width="@dimen/ds_l" android:layout_height="@dimen/ds_l" android:layout_marginTop="@dimen/ds_s" android:src="@drawable/ic_filled_heart" android:layout_marginEnd="@dimen/ds_xl" app:layout_constraintEnd_toEndOf="0" app:layout_constraintTop_toTopOf="0"/>
    <LinearLayout android:orientation="vertical" android:id="@+id/vendor_content_ll" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingStart="@dimen/ds_s" android:paddingEnd="@dimen/ds_s" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/headerContainer">
