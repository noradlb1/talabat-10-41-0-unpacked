                    <androidx.appcompat.widget.AppCompatImageView android:layout_gravity="center" android:id="@+id/parallel_bin_image" android:layout_width="@dimen/ds_m" android:layout_height="@dimen/ds_m" app:tint="@color/ds_promo_100"/>
