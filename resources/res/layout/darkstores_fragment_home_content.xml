            <LinearLayout android:orientation="vertical" android:id="@+id/header_title_search" android:background="@color/ds_neutral_white" android:paddingTop="@dimen/ds_s" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintBottom_toBottomOf="@+id/backgroundImage" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0">
                <include android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginStart="@dimen/ds_s" android:layout_marginEnd="@dimen/ds_s" layout="@layout/darkstores_search_field_non_editable"/>
