    <androidx.recyclerview.widget.RecyclerView android:id="@+id/homeRecyclerView" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <ImageView android:layout_gravity="bottom|right|center_vertical|center_horizontal|center" android:id="@+id/talabatAiButton" android:visibility="gone" android:layout_width="@dimen/talabat_ai_button_size" android:layout_height="@dimen/talabat_ai_button_size" android:layout_margin="@dimen/ds_s" android:src="@drawable/ic_talabat_ai"/>
