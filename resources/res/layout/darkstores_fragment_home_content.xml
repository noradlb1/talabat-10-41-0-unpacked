                <View android:background="@color/ds_neutral_10" android:layout_width="match_parent" android:layout_height="@dimen/divider_height" android:layout_alignParentBottom="true"/>
            </LinearLayout>
            <include android:id="@+id/vouchers_swimlane" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" layout="@layout/darkstores_view_customer_vouchers_swimlane"/>
