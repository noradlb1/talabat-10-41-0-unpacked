<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent" app:layout_behavior="@string/appbar_scrolling_view_behavior">
    <com.talabat.darkstores.common.customScrollingViews.CustomVerticalNestedScrollView android:id="@+id/darkstore_home_container" android:background="@color/ds_neutral_white" android:layout_width="match_parent" android:layout_height="match_parent">
