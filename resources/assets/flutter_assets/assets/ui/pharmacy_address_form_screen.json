        "autovalidateMode": "onUserInteraction",
        "hint": null,
        "prefixSearchEmptyStateTitle": "Nothing found",
        "prefixSearchEmptyStateDetails": "Modify your search to find what you're looking for",
        "prefixSearchHint": "Search by country or code"
      }
    },
    "building_apartment": {
      "type": "InputTextFormField",
      "data": {
        "title": "Building name",
        "value": "",
        "input_type": "streetAddress",
        "max_chars": 50,
        "validation": [
          {
            "error_message": "",
            "regex": "^.+$"
          }
        ],
        "autovalidateMode": "onUserInteraction",
        "request_key": "building_no",
        "hint": null,
        "input_action": "next"
      }
    },
    "apartment": {
      "type": "InputTextFormField",
      "flex": 12,
      "data": {
        "title": "Apt. no.",
        "value": "",
        "input_type": "streetAddress",
        "max_chars": 30,
        "validation": [
          {
            "error_message": "",
            "regex": "^.+$"
          }
        ],
        "autovalidateMode": "onUserInteraction",
        "request_key": "suite",
        "hint": null,
        "input_action": "next"
      }
    },
    "floor_apartment": {
      "type": "InputTextFormField",
      "flex": 12,
      "data": {
        "title": "Floor",
        "value": "",
        "input_type": "streetAddress",
        "max_chars": 30,
        "validation": [
          {
            "error_message": "",
            "regex": "^.+$"
          }
        ],
        "autovalidateMode": "onUserInteraction",
        "request_key": "floor",
        "hint": null,
        "input_action": "next"
      }
